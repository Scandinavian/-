from math import sqrt
a = float(input())
b = float(input())
c = float(input())
D = b ** 2 - 4 * a * c
if D < 0:
    print('')
elif D == 0:
    s = sqrt(D)
    x = (- b + s) / (2 * a)
    print(x)
elif D > 0:
    s = sqrt(D)
    x1 = (- b + s) / (2 * a)
    x2 = (- b - s) / (2 * a)
    if x1 > x2:
        print(x2, x1)
    else:
        print(x1, x2)
