# first method

n = int(input())
i = 1
while i <= n:
    i = i + 1
    if n % i == 0:
        print(i)
        break

# second method

from math import sqrt


def MinDivisor(n):
    i = 2
    while n % i != 0:
        if i >= sqrt(n):
            print(n)
            return
        i = i + 1
    print(i)


n1 = int(input())
(MinDivisor(n1))
